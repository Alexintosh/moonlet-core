{
  "name": "wws-core.js",
  "version": "0.0.1",
  "description": "Wallet Suite - Core Logic Package",
  "main": "wws-core.js",
  "types": "wws-core.d.ts",
  "scripts": {
    "build": "tsc -p . && gulp build",
    "build-lib": "tsc -p .",
    "coverage": "scripts/run_coverage.sh all",
    "coverage-reuse": "scripts/run_coverage.sh all use-existing",
    "test": "scripts/run_tests.sh all",
    "test-reuse": "scripts/run_tests.sh all use-existing",
    "test-single": "scripts/run_tests.sh single",
    "test-single-reuse": "scripts/run_tests.sh single use-existing",
    "testToHtml": "scripts/testOutputToHtml.sh",
    "start-all-rpcs": "scripts/rpcs/start_all.sh",
    "stop-all-rpcs": "scripts/rpcs/stop_all.sh"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": "src",
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html",
      "text"
    ],
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cryptolandtech/wws-core"
  },
  "keywords": [
    "wws-core",
    "crypto",
    "wallet"
  ],
  "author": "Micky Socaci <micky@nowlive.ro>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cryptolandtech/wws-core/issues"
  },
  "homepage": "https://github.com/cryptolandtech/wws-core#readme",
  "dependencies": {
    "axios": "^0.16.2",
    "babel-core": "^6.26.3",
    "babelify": "^8.0.0",
    "bcrypto": "^1.1.0",
    "bip39": "^2.5.0",
    "bn.js": "^4.11.8",
    "console-error": "0.0.4",
    "console-info": "0.0.5",
    "console-warn": "0.0.4",
    "ethereumjs-tx": "^1.3.7",
    "ethereumjs-util": "^5.2.0",
    "fs": "0.0.1-security",
    "generichd-wallet": "github:cryptolandtech/generichd-wallet",
    "gulp": "^3.9.1",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-uglify-es": "^1.0.4",
    "gulp-util": "^3.0.8",
    "hash.js": "^1.1.5",
    "hdkey": "^1.1.0",
    "https": "^1.0.0",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "zilliqa-js": "^0.1.8"
  },
  "devDependencies": {
    "@types/chai": "^4.1.5",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.11.0",
    "browserify": "^16.2.3",
    "chai": "^4.1.2",
    "compression-webpack-plugin": "^1.1.12",
    "ganache-cli": "^6.1.8",
    "istanbul": "^0.4.5",
    "kaya": "git+https://github.com/cryptolandtech/kaya.git",
    "mocha": "^4.1.0",
    "mocha-loader": "^1.1.3",
    "mocha-webpack": "^1.0.1",
    "mochawesome": "^2.3.1",
    "nyc": "^13.1.0",
    "safe-buffer": "^5.1.2",
    "source-map-support": "^0.5.9",
    "ts-mocha": "^1.3.0",
    "typescript": "^2.8.3",
    "webpack": "^3.11.0",
    "webpack-bundle-analyzer": "^2.11.0",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.6.0"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "standard": {
    "env": "mocha",
    "globals": [
      "describe",
      "it"
    ]
  }
}
